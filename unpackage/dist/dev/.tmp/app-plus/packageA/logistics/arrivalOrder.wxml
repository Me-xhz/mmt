<view class="arrival-order"><view class="arrival-header"><view class="order-number">{{boxInfo.count}}</view><view class="order-number-title">{{boxInfo.box_name+"箱子订单数"}}</view></view><view class="order-list"><block wx:for="{{items}}" wx:for-item="item" wx:for-index="index" wx:key="index"><view class="order-item"><view class="order-item-text">{{"物流单号："+item.delivery_sn}}</view><view class="order-item-text">{{"订单号："+item.order_sn}}</view><view class="order-item-text">{{"收货人："+item.consignee}}</view><view class="order-item-text">{{"手机号："+item.mobile}}</view><view class="order-item-text">{{"装箱时间："+item.add_time}}</view></view></block></view></view>