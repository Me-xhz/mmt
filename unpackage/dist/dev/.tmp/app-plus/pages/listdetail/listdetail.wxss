.goodsCard {
  position: relative;
  width: 50%;
  display: inline-block;
  -webkit-box-sizing: border-box;
          box-sizing: border-box;
  font-size: 0;
  background: #fff;
  padding-right: 2rpx;
}
.goodsCard.goodsCard1 {
  float: right;
  border: 0
}
.goodsCard .images {
  position: relative;
  width: 100%;
  padding-top: 100%;
  background-position: 50%;
  background-size: cover;
  background-repeat: no-repeat;
  background-color: rgb(247, 244, 244);
}
.goodsCard .goodsName {
  width: 100%;
  height: 110rpx;
  line-height: 38rpx;
  font-family: PingFang;
  display: inline-block;
  -webkit-box-sizing: border-box;
          box-sizing: border-box;
  text-align: left;
  overflow: hidden;
  padding:32rpx 24rpx 8rpx 24rpx;
  -webkit-box-orient:vertical;
  -webkit-line-clamp:2;
  white-space:normal;
  -o-text-overflow:inherit;
     text-overflow:inherit;
  display:-webkit-box;
}
.goodsCard .price {
  padding: 10rpx 0 40rpx 24rpx;
}
.goodsCard .price .shop_price{
  font-weight: 600
}
.goodsCard .price .market_price {
  text-decoration: line-through;
  margin-left:16rpx;
}
.Videoimg{
  position: absolute;
  top: 20rpx;
  right: 20rpx;
}
.Videoimg image{
  width: 88rpx;
  height: 32rpx;
}
page {
  height: 100%;
  background: white;
}
.titleImg {
  width: 100%;
  height: 400rpx;
  background-color: #fff;
  overflow: hidden;
}
.titleImg image {
  width: 100%;
  height: 400rpx;
  vertical-align: middle;
}
.titleText {
  position: relative;
  display: block;
  font-size: 36rpx;
  line-height: 43rpx;
  color: #010101;
  padding: 26rpx 171rpx 26rpx 20rpx;
  background: #fff;
}
.titleDescribe {
  position: relative;
  display: block;
  background: #fff;
  padding-bottom: 50rpx;
  /* border-bottom: 2rpx #f2f2f2 solid; */
}
.describe {
  overflow: hidden;
  -o-text-overflow: ellipsis;
     text-overflow: ellipsis;
  display: -webkit-box;
  -webkit-box-orient: vertical;
  color: #7f7f7f;
  font-size: 28rpx;
  -webkit-line-clamp: 100;
  line-height: 36rpx;
  padding: 0 30rpx 0 20rpx;
  text-align: left;
}
.describe.cut {
  -webkit-line-clamp: 2;
}
.attr-shadow {
  display: none;
  position: absolute;
  content: "";
  width: 100%;
  height: 100%;
  bottom: 0;
  left: 0;
  background-image: -webkit-gradient(linear, left top, left bottom, from(rgba(255, 255, 255, 0)), to(white));
  z-index: 1;
}
.attr-shadow.show{
  display: block;
}
.limit-words {
  position: absolute;
  bottom: 5rpx;
  left: 50%;
  -webkit-transform: translateX(-50%);
      -ms-transform: translateX(-50%);
          transform: translateX(-50%);
  display: block;
  width: 100%;
  font-size: 24rpx;
  line-height: 50rpx;
  color: #767676;
  text-align: center;
  z-index: 2;
}
.limit-words image {
  display: inline-block;
  margin-left: 6rpx;
  width: 30rpx;
  height: 18rpx;
  background-repeat: no-repeat;
  background-size: 32rpx 18rpx;
}
.limit-words .show {
  background-image: url("data:image/png;base64,");
}
.limit-words .hidden {
  background-image: url("data:image/png;base64,");
}
.brand_more {
  position: absolute;
  right: 20rpx;
  color: #767676;
  font-size: 28rpx;
  top: 50%;
  -ms-transform: translateY(-50%);
      transform: translateY(-50%);
  -webkit-transform: translateY(-50%);
}
.brand_more .icon-right-arrow {
  width: 14rpx;
  height: 28rpx;
  padding-left: 10rpx;
}
.list {
  background: #fff;
  width: 100%;
}
.list .mod-margin {
  padding-bottom: 80rpx;
}
.list .mod-nav {
  width: 100%;
  height: 80rpx;
  position: fixed;
  z-index: 2;
  background-color: #fff;
  -webkit-box-sizing: border-box;
          box-sizing: border-box;
  border-bottom: 1rpx solid #e9e9e9;
}
.list .nav-ul {
  display: block;
  white-space: nowrap;
  height: 100%;
}
.list .nav-ul ._li {
  height: 100%;
  font-size: 28rpx;
  margin: 0 20rpx;
}
.list .nav-ul ._li ._span {
  display: inline-block;
  padding: 0 10rpx;
  line-height: 80rpx;
  position: relative;
}
.actives {
  color: #ff7f95;
}
.actives ._span::before {
  content: '';
  position: absolute;
  bottom: 0;
  left: 0;
  right: 0;
  height: 8rpx;
  background-color: #ff7f95;
}
.cate-list-more {
  display: block;
  width: 100%;
  height: 99rpx;
  line-height: 99rpx;
  -webkit-box-sizing: border-box;
          box-sizing: border-box;
  color: #999;
  font-size: 26rpx;
  font-weight: 400;
  text-align: center;
}

