  border-radius: 8rpx;
  padding: 45rpx 25rpx;
  overflow: hidden;
  position: relative;
}
.list-item {
  padding: 10rpx 0;
  color: #555555;
  font-size: 30rpx;
}
.list-time {
  position: absolute;
  right: 0;
  top: 0;
  background-color: #1DA1F2;
  color: #ffffff;
  padding: 10rpx;
  font-size: 24rpx;
}
