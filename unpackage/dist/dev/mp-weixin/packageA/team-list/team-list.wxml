<view><block wx:if="{{isShowGoHome}}"><view data-event-opts="{{[['tap',[['gotoHome',['$event']]]]]}}" class="goHomeFixed" catchtap="__e"><image class="icon-leftReturn" src="{{iconURL+'/product-icon-leftReturn.svg'}}"></image><text>返回首页</text></view></block><view class="join-team"><view class="team-list-box"><view class="team-list"><block wx:for="{{teamList}}" wx:for-item="item" wx:for-index="index" wx:key="index"><view><view class="join-list"><image class="join-list-img" src="{{item.head_pic}}"></image><view class="join-list-name">{{''+item.nickname+''}}</view><view class="join-list-right"><view class="join-list-less"><view>还差<text class="i">{{item.surplus+"人"}}</text>拼成</view><view class="count-time">剩余<count-down vue-id="{{'1-'+index}}" time-text="time-text" doc-text="doc-text" timestamp="{{item.found_end_time*1000}}" type="2" bind:__l="__l"></count-down></view></view><form report-submit="true" data-alpha-beta="0" data-found_id="{{item.found_id}}" data-team_id="{{item.team_id}}" data-event-opts="{{[['submit',[['toAddTeam',['$event']]]]]}}" bindsubmit="__e"><button class="join-list-btn" form-type="submit">去拼团</button></form></view></view></view></block></view></view></view></view>