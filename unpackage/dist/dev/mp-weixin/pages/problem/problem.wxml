<view class="problemBox"><scroll-view class="outermost" scroll-y="{{true}}"><view class="problem"><block wx:for="{{questionList.textList}}" wx:for-item="item" wx:for-index="index" wx:key="index"><view class="problem-block"><view class="question-name row" data-id="{{index}}" data-event-opts="{{[['tap',[['showToggle',['$event']]]]]}}" bindtap="__e"><view class="contant-lf"><text class="problemTitle">{{item.name}}</text><text class="subtitle">{{item.title}}</text></view><view class="contant-rf"><view class="{{['iconfont icon-btn_xiangxiajiantouhongx down '+(item.isSubShow?'uprotate':'uprotate2')+' ']}}"></view></view></view><block wx:if="{{item.isSubShow}}"><view class="second-list"><block wx:for="{{item.subItems}}" wx:for-item="subItem" wx:for-index="index2" wx:key="index2"><view><view class="question-row row" data-id="{{index}}" data-event-opts="{{[['tap',[['showThirdToggle',['$event']]]]]}}" bindtap="__e"><text class="subText">{{subItem.name+''}}</text><view class="{{['iconfont icon-btn_xiangyoudajiantoux right '+(subItem.isThirdShow?'downRotate':'downRotate2')]}}"></view></view><block wx:if="{{subItem.isThirdShow}}"><view class="third-list"><block wx:for="{{subItem.threeItem}}" wx:for-item="third" wx:for-index="index" wx:key="index"><view class="third-question"><text class="subText width647">{{third.answer}}</text></view></block></view></block></view></block></view></block></view></block></view><view class="problem"><block wx:for="{{questionList.videoList}}" wx:for-item="item" wx:for-index="index" wx:key="index"><view class="problem-block"><view class="question-name row" data-src="{{item.src}}" data-event-opts="{{[['tap',[['openVideo',['$event']]]]]}}" bindtap="__e"><view class="video-lf"><text class="problemTitle">{{item.name}}</text><text class="subtitle">{{item.title}}</text></view><view class="video-rf"><view class="right-play color_theme iconfont icon-btn_bofangx" style="color:#555;"></view></view></view></view></block></view></scroll-view><view class="serveBox"><text class="serveTime">服务时间：9:00-18:00</text><view data-event-opts="{{[['tap',[['callPhone',['$event']]]]]}}" class="contact" bindtap="__e"><text class="text">联系客服:0591-62751447</text></view><view data-event-opts="{{[['tap',[['toFeedBook',['$event']]]]]}}" class="confirm-btn" bindtap="__e"><text class="txt">留言反馈</text></view></view><block wx:if="{{showVideo}}"><view class="full-video"><video src="{{videoSrc}}" autoplay="true"></video><view data-event-opts="{{[['tap',[['closeVideo',['$event']]]]]}}" class="video-close iconfont icon-btn_qiapianguanbianniux" bindtap="__e"></view></view></block></view>